generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Product {
  id          Int      @id @default(autoincrement())
  name        String
  description String
  price       Float
  modelUrl    String
  createdAt   DateTime @default(now())
}

model FeaturedCar {
  id         Int      @id @default(autoincrement())
  modelUrl   String
  uploadedAt DateTime @default(now())
}